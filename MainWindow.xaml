<Window x:Class="Baibakov4122UfanetCourseWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Baibakov4122UfanetCourseWork"
        mc:Ignorable="d"
        Title="Baibakov Rustam" Height="450" Width="800" FontFamily="Century Gothic" Icon="Resources/logo_png.png" MinWidth="800" MinHeight="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Image Source="Resources/logo_png.png" HorizontalAlignment="Left"/>
        <TextBlock Text="Список клиентов" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        <Frame Name="MainFrame" Grid.Row="1" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>

        <Button Name="back_btn" Content="Назад" HorizontalAlignment="Right" Click="back_btn_Click" Style="{StaticResource RoundedButtonStyle}"/>
    </Grid>
</Window>
